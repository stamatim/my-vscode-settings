m\\n\",\n      \"\\u001b[1;31mNameError\\u001b[0m: name 'numpy' is not defined\"\n     ]\n    }\n   ],\n   \"source\": [\n    \"# Each sample is 2-D\\n\",\n    \"X0 = numpy.hstack((X1, X2)) \\n\",\n    \"# now each row is a feature, each column is a sample\\n\",\n    \"X = numpy.vstack((X0, numpy.ones(2*N))) # augment\\n\",\n    \"y = numpy.hstack((numpy.ones(N)*-1, 1*numpy.ones(N)))\\n\",\n    \"print (X.shape)\"\n   ]\n  },\n  {\n   \"cell_type\": \"code\",\n   \"execution_count\": 11,\n   \"metadata\": {},\n   \"outputs\": [],\n   \"source\": [\n    \"def plot_mse(X, y):\\n\",\n    \"    w = np.array([0,0,0]) \\n\",\n    \"    compound = numpy.matmul(X, numpy.transpose(X))\\n\",\n    \"    all_but_y = numpy.matmul(numpy.linalg.inv(compound), X)\\n\",\n    \"    W = numpy.matmul(all_but_y, y)\\n\",\n    \"    a,b,c=W[0], W[1], W[2]\\n\",\n    \"    h = [0, -1*c/a]\\n\",\n    \"    vd = [-1*c/b, 0]\\n\",\n    \"\\n\",\n    \"    colors = {1: 'red', 2: 'blue'}\\n\",\n    \"\\n\",\n    \"    def plot_x_y(title=\\\"X vs Y: Linear Regression\\\", ax=None):\\n\",\n    \"        if ax == None:\\n\",\n    \"            ax = plt.gca()\\n\",\n    \"        \\n\",\n    \"        for i in range(N):\\n\",\n    \"            ax.scatter(X1[0], X1[1], color=colors[1])\\n\",\n    \"            ax.scatter(X2[0], X2[1], color=colors[2])\\n\",\n    \"         \\n\",\n    \"        ax.set_title(title)\\n\",\n    \"        ax.set_xlabel('X1')\\n\",\n    \"        ax.set_ylabel('X2')\\n\",\n    \"        ax.grid()\\n\",\n    \"\\n\",\n    \"    plt.figure(figsize=(8,8))\\n\",\n    \"    plot_x_y()\\n\",\n    \"    plt.plot(h, vd)\\n\",\n    \"\\n\",\n    \"    plt.show()\"\n   ]\n  },\n  {\n   \"cell_type\": \"code\",\n   \"execution_count\": 12,\n   \"metadata\": {},\n   \"outputs\": [\n    {\n     \"output_type\": \"error\",\n     \"ename\": \"NameError\",\n     \"evalue\": \"name 'X' is not defined\",\n     \"traceback\": [\n      \"\\u001b[1;31m--------------------------------------